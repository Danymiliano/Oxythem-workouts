    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.1.5",
    "react-redux": "^8.0.5",
    "yup": "^1.0.0"
  },
  "devDependencies": {
    "@next/font": "13.1.6",
    "@svgr/webpack": "^6.5.1",
    "@types/node": "18.13.0",
    "@types/react": "18.0.28",
